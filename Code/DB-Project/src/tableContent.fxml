<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TableController">
   <HBox prefHeight="100.0" prefWidth="200.0">
      <children>
          <Button fx:id="addRowButt" onAction="#addRow" text="Add Row" />
          <Button fx:id="addColumnButt" onAction="#addColumn" text="Add Column" />
      </children>
   </HBox>
    <TableView fx:id="tableView" editable="true" layoutY="30.0" prefHeight="470.0" prefWidth="620.0">
    </TableView>
</AnchorPane>
