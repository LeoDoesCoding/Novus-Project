<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.CRUDinator.TableController">
   <HBox prefHeight="100.0" prefWidth="200.0">
      <children>
          <Button fx:id="addRowButt" minWidth="3.0" onAction="#addRow" text="Add Row" />
          <Button fx:id="addColumnButt" minWidth="30.0" onAction="#addColumn" text="Add column" />
      </children>
   </HBox>
    <TableView fx:id="tableView" editable="true" layoutY="30.0" maxHeight="400" prefWidth="620.0">
    </TableView>
</AnchorPane>
