<?import javafx.scene.layout.Pane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>

<!--issatable-->
<Pane prefHeight="292.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TableController">
    <children>
        <Button text="Add Row" layoutY="100" layoutX="650" onAction="#addRow"/>
        <Button text="New column" layoutY="130" layoutX="650" onAction="#addColumn"/>
        <TableView fx:id="tableView" prefHeight="500.0" prefWidth="620.0" editable="true">
        </TableView>
    </children>
</Pane>